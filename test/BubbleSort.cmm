/* Ordenação para inteiros */
int ordenarInteger(int *vetor, int tam){
    int i;
    int j;
    bool trocou;
    int aux;
    i=0;
    while(i<tam-1){
        j=0;
        trocou=false;
        while(j<tam-1-i){
            if(vetor[j] > vetor[j+1]){
                aux = vetor[j];
                vetor[j] = vetor[j+1];
                vetor[j+1] = aux;
                trocou=true;
            }
            j=j+1;
        }
        if(trocou==false){
            break;
        }
        i=i+1;
    }
    return 0;
}

int main() {
    // Vetor a ordenar
    int teste[5];
    int i;
    teste={5,0,1,4,3};
    i=0;
    while(i<5){
        print("%d ", teste[i]);
        i=i+1;
    }
    print("\n");
    ordenarInteger(teste, 5);
    i=0;
    // Imprime o vetor ordenado
    while(i<5){
        print("%d ", teste[i]);
        i=i+1;
    }
    print("\n");
    return 0;
}